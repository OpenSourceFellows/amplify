{
  "private": true,
  "name": "amplify",
  "version": "1.0.0",
  "description": "The frontend and API backend for ProgramEquity",
  "author": "ProgramEquity",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ProgramEquity/amplify.git"
  },
  "bugs": {
    "url": "https://github.com/ProgramEquity/amplify/issues"
  },
  "homepage": "https://www.programequity.com/",
  "main": "server/server.js",
  "scripts": {
    "postinstall": "husky install",
    "start": "node server/server.js",
    "dev": "cross-env NODE_ENV=development concurrently -n frontend,backend \"npm run serve\" \"npm:dev:backend\"",
    "dev:backend": "cross-env PORT=5000 nodemon -w ./server server/server.js",
    "lint": "cross-env DEBUG=eslint:cli-engine eslint --fix .",
    "lint:check": "cross-env DEBUG=eslint:cli-engine eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --detectOpenHandles --forceExit",
    "precommit": "npm run lint:check && npm run format:check",
    "db:create": "npm run db:create:dev && npm run db:create:test",
    "db:create:dev": "node script/create-db.js --env development",
    "db:create:test": "node script/drop-db.js --env test && node script/create-db.js --env test",
    "db:migrate": "npm run db:migrate:dev && npm run db:migrate:test",
    "db:migrate:dev": "knex migrate:latest --verbose --env development",
    "db:migrate:test": "knex migrate:latest --verbose --env test",
    "db:migrate:prod": "knex migrate:latest --verbose --env production",
    "db:seed": "knex seed:run",
    "heroku-postbuild": "npm run build && npm run db:migrate:prod",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.2",
    "@babel/polyfill": "^7.4.4",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^2.0.8",
    "@stripe/stripe-js": "^1.35.0",
    "@vue-stripe/vue-stripe": "^4.4.4",
    "axios": "^0.27.2",
    "axios-cache-interceptor": "^0.10.7",
    "bcrypt": "^5.1.1",
    "cli-plugin-eslint": "^0.1.0",
    "connect-history-api-fallback": "^2.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-jwt": "^8.4.0",
    "express-rate-limit": "^6.6.0",
    "jwks-rsa": "^2.1.4",
    "knex": "^2.4.2",
    "lob": "^6.6.3",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "register-service-worker": "^1.7.1",
    "stripe": "^11.1.0",
    "twilio": "^4.19.3",
    "vue": "^2.6.11",
    "vue-logger-plugin": "^1.2.3",
    "vue-router": "^3.6.5",
    "vuetify": "^2.6.8",
    "vuex": "^3.4.0",
    "winston": "^3.8.2",
    "winston-transport-browserconsole": "^1.0.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@vue/cli-plugin-babel": "~5.0.4",
    "@vue/cli-plugin-pwa": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "less": "^4.1.3",
    "less-loader": "^7.3.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "style-resources-loader": "^1.4.1",
    "supertest": "^6.3.0",
    "vue-cli-plugin-style-resources-loader": "~0.1.5",
    "vue-eslint-parser": "^8.3.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 10"
  ]
}
