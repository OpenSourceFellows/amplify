{
  "private": true,
  "name": "amplify",
  "version": "1.0.0",
  "description": "The  frontend and API backend for ProgramEquity",
  "author": "Ally Guide",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ally-Guide/amplify-back-end.git"
  },
  "bugs": {
    "url": "https://github.com/Ally-Guide/amplify-back-end/issues"
  },
  "homepage": "https://www.programequity.com/",
  "main": "server/server.js",
  "scripts": {
    "postinstall": "husky install",
    "be-start": "node server/server.js",
    "be-dev": "nodemon server/dev-server.js",
    "start": "npm run build && npm run be-start",
    "dev": "concurrently \"npm run be-dev\" \"npm run fe-dev\"",
    "lint": "eslint --fix .",
    "lint:check": "eslint .",
    "format": "prettier --write . '!**/*.md'",
    "format:check": "prettier --check . '!**/*.md'",
    "test": "jest --detectOpenHandles --forceExit",
    "precommit": "npm run lint:check && npm run format:check",
    "db:create": "npm run db:create:dev && npm run db:create:test",
    "db:create:dev": "node script/create-db.js --env development",
    "db:create:test": "node script/drop-db.js --env test && node script/create-db.js --env test",
    "db:migrate": "npm run db:migrate:dev && npm run db:migrate:test",
    "db:migrate:dev": "knex migrate:latest --verbose --env development",
    "db:migrate:test": "knex migrate:latest --verbose --env test",
    "db:migrate:prod": "knex migrate:latest --verbose --env production",
    "db:seed": "knex seed:run",
    "heroku-postbuild": "npm run db:migrate:prod",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "fe-dev": "npm run serve"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.17.0",
    "@babel/polyfill": "^7.4.4",
    "@stripe/stripe-js": "^1.22.0",
    "@vue-stripe/vue-stripe": "^4.4.1",
    "axios": "^0.24.0",
    "cli-plugin-eslint": "^0.1.0",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-jwt": "^6.0.0",
    "express-rate-limit": "^6.2.1",
    "jwks-rsa": "^2.0.3",
    "knex": "^0.95.15",
    "lob": "^6.2.0",
    "pg": "^8.6.0",
    "register-service-worker": "^1.7.1",
    "stripe": "^8.202.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuetify": "^2.6.3",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "jest": "^27.4.7",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "nodemon": "^2.0.7",
    "prettier": "2.5.1",
    "sass": "^1.32.0",
    "sass-loader": "^10.0.0",
    "style-resources-loader": "^1.4.1",
    "supertest": "^6.1.3",
    "vue-cli-plugin-style-resources-loader": "~0.1.5",
    "vue-eslint-parser": "^8.2.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.7.0"
  },
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true,
    "useTabs": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 10"
  ]
}
